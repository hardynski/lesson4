pipeline {
    agent any
    stages {
        stage('Clone repository') {
          steps {
               checkout scm
               }
            }
        stage('Change directory') {
          steps {
               sh "pwd"
               dir('lesson7_Jenk') {
               sh "pwd"
               }
            } 
            }


        stage('Docker Build') {
          steps {
               sh "pwd"
               dir('lesson7_Jenk') {
               sh "pwd"
                  }
               sh 'docker build -t ubuntu:v1 .'
      }
    }
          }
}
